import javax.swing.*;
import java.awt.*;
import java.awt.event.*;
import javax.swing.event.*;


public class Dot {

	private Color color;
	private int x, y;
	private int RADIUS = 3;
	
	
	public Dot(int x1, int y1, Color color1) {
		
		color = color1;
		x = x1;
		y = y1;
	
	}
	
	
	public void draw(Graphics page) {
		page.fillOval(x, y, RADIUS, RADIUS);
	}
	
	
	
	
	
	
}
